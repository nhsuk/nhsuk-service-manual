{
  "name": "nhsuk-service-manual",
  "version": "8.2.0",
  "description": "NHS digital service manual",
  "engines": {
    "node": "^20.9.0 || ^22.11.0",
    "npm": "^10.5.0"
  },
  "main": "app.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "NODE_ENV=${NODE_ENV:-production} webpack",
    "clean": "del-cli public/**",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:prettier",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:prettier:fix",
    "lint:prettier": "prettier --cache --cache-location .cache/prettier --cache-strategy content --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:js": "eslint --cache --cache-location .cache/eslint --cache-strategy content --color --ignore-path .gitignore --max-warnings 0 \"**/*.{cjs,js,md,mjs}\"",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint --cache --cache-location .cache/stylelint --cache-strategy content --color --formatter verbose --ignore-path .gitignore --max-warnings 0 \"**/*.{md,scss}\"",
    "lint:css:fix": "npm run lint:css -- --fix",
    "nuke": "npm run clean && rm -rf node_modules/",
    "start": "node .",
    "pretest": "npm run lint && npm run build",
    "test": "npm run test:js",
    "test:js": "jest --color",
    "watch": "concurrently --prefix none npm:watch:*",
    "watch:build": "NODE_ENV=development npm run build -- --watch",
    "watch:start": "NODE_ENV=development node --watch .",
    "prepare": "husky"
  },
  "author": "https://github.com/nhsuk",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhsuk/nhsuk-service-manual.git"
  },
  "license": "MIT",
  "dependencies": {
    "@open-iframe-resizer/core": "^2.0.0",
    "accessible-autocomplete": "^3.0.1",
    "axios": "^1.12.2",
    "basic-auth": "^2.0.1",
    "browser-sync": "^3.0.4",
    "cheerio": "^1.0.0",
    "clipboard": "^2.0.11",
    "compression": "^1.8.1",
    "express": "^4.21.2",
    "gray-matter": "^4.0.3",
    "helmet": "^4.6.0",
    "highlight.js": "^11.11.1",
    "js-beautify": "^1.15.4",
    "lunr": "^2.3.9",
    "marked": "^15.0.11",
    "nhsuk-frontend": "^10.1.0",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.41.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.0",
    "cssnano": "^7.1.2",
    "del-cli": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-es-x": "^8.7.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsdoc": "^60.7.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "postcss": "^8.5.4",
    "postcss-load-config": "^6.0.1",
    "postcss-loader": "^8.1.1",
    "postcss-markdown": "^1.3.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.3",
    "sass-loader": "^16.0.6",
    "stylelint": "^16.24.0",
    "stylelint-config-gds": "^2.0.0",
    "stylelint-order": "^7.0.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-manifest-plugin": "^6.0.0"
  },
  "files": [
    "app",
    "lib",
    "middleware",
    "public",
    "app.js",
    "!**/*.test.*"
  ]
}
